{
   "name": "@apocaliss92/scrypted-reolink-utilities",
   "description": "Scrypted plug in to add capabilities to Reolink cameras",
   "repository": {
      "type": "git",
      "url": "https://github.com/apocaliss92/scrypted-reolink-utilities"
   },
   "version": "0.0.1",
   "scripts": {
      "scrypted-setup-project": "scrypted-setup-project",
      "prescrypted-setup-project": "scrypted-package-json",
      "build": "scrypted-webpack",
      "prepublishOnly": "NODE_ENV=production scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-readme": "scrypted-readme",
      "scrypted-package-json": "scrypted-package-json"
   },
   "keywords": [
      "scrypted",
      "plugin",
      "reolink",
      "utilities",
      "videoclips",
      "snapshots",
      "battery"
   ],
   "scrypted": {
      "name": "Reolink utilities",
      "type": "API",
      "interfaces": [
         "Settings",
         "MixinProvider",
         "HttpRequestHandler"
      ],
      "pluginDependencies": []
   },
   "dependencies": {
      "@scrypted/sdk": "^0.3.65",
      "axios": "^1.7.7",
      "lodash": "^4.17.21",
      "mqtt": "^4.2.8"
   },
   "devDependencies": {
      "@types/lodash": "^4.17.12",
      "@types/node": "^20.11.0"
   }
}
